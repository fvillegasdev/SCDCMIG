@using EK.Common.Reportes
@{
    Layout = "~/Views/Shared/_R3LetterPortrait.cshtml";

    dynamic data = ViewBag.Data;

    var reporte = new EK.Common.Reportes.Reporte();
    reporte.Titulo = "MONEDAS";
    reporte.SubTitulo = "";
    reporte.FechaImpresion = System.DateTime.Now.ToString();
    reporte.Columnas = new List<Reporte.Columna>();
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Clave", Longitud = 150, Campo = "Clave" });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Nombre", Longitud = 400, Campo = "Nombre" });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Estatus", Longitud = 150, Campo = "Estatus.Nombre" });
    ViewBag.Reporte = reporte;
}
