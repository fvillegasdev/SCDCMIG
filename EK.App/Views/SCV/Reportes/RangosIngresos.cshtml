@using EK.Common.Reportes
@{
    Layout = "~/Views/Shared/_R3LetterPortrait.cshtml";

    dynamic data = ViewBag.Data.Resultado;

    var reporte = new EK.Common.Reportes.Reporte();
    reporte.Titulo = "Catálogo de Rangos de Ingresos";
    reporte.SubTitulo = "HAVITAL COMERCIAL, S.A. DE C.V.";
    reporte.FechaImpresion = System.DateTime.Now.ToString();

    reporte.Columnas = new List<Reporte.Columna>();
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Clave", Longitud = 50, Campo = "Clave" });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Nombre", Longitud = 200, Campo = "Nombre" });    
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Rango Inicial", Longitud = 100, Campo = "RangoInicial" });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Rango Final", Longitud = 100, Campo = "RangoFinal" });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Estatus", Longitud = 50, Campo = "Estatus.Nombre" });

    ViewBag.Reporte = reporte;
}