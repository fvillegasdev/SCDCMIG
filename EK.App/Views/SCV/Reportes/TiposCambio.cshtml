@using EK.Common.Reportes
@{
    Layout = "~/Views/Shared/_R3LetterPortrait.cshtml";

    dynamic data = ViewBag.Data.Resultado;

    var reporte = new EK.Common.Reportes.Reporte();
    reporte.Titulo = "Catálogo de Tipos de Cambio";
    reporte.SubTitulo = "HAVITAL COMERCIAL, S.A. DE C.V.";
    reporte.FechaImpresion = System.DateTime.Now.ToString();

    reporte.Columnas = new List<Reporte.Columna>();
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Clave", Campo = "Moneda.Clave", Longitud = 100 });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Moneda", Campo = "Moneda.Nombre", Longitud = 200 });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Fecha", Campo = "Fecha", Longitud = 200 });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Tipo de Cambio", Campo = "TiposCambio", Longitud = 150 });
    reporte.Columnas.Add(new Reporte.Columna() { Titulo = "Estatus", Campo = "Estatus.Nombre", Longitud = 150 });

    ViewBag.Reporte = reporte;
}